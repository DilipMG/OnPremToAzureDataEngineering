{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "onprem-to-azure-dmg-df"
		},
		"ADLS2_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS2_ls'"
		},
		"ADLS2_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://onpremtoazureadls2.dfs.core.windows.net/"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://omprem-to-azure-dmg-kv.vault.azure.net/"
		},
		"onepremsqlserver_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "VRUDDHI"
		},
		"onepremsqlserver_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorks2017"
		},
		"onepremsqlserver_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dilip"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "onprem-to-azure-dmg-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS2_ls_accountKey": {
						"value": "[parameters('ADLS2_ls_accountKey')]"
					},
					"ADLS2_ls_properties_typeProperties_url": {
						"value": "[parameters('ADLS2_ls_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"onepremsqlserver_ls_properties_typeProperties_server": {
						"value": "[parameters('onepremsqlserver_ls_properties_typeProperties_server')]"
					},
					"onepremsqlserver_ls_properties_typeProperties_database": {
						"value": "[parameters('onepremsqlserver_ls_properties_typeProperties_database')]"
					},
					"onepremsqlserver_ls_properties_typeProperties_userName": {
						"value": "[parameters('onepremsqlserver_ls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}